<?xml version="1.0" encoding="UTF-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="de.timoklostermann.refuel">

    <uses-sdk android:minSdkVersion="14" android:targetSdkVersion="15"/>

    <uses-permission android:name="android.permission.INTERNET"/>

   <application android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:label="@string/app_name" android:name=".SwipeActivity"/>
        <activity android:label="@string/title_activity_login" android:name=".LoginActivity" android:noHistory="true">
            <intent-filter android:label="@string/app_name">
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/title_activity_register" android:name=".RegisterActivity" android:noHistory="true"/>
        <activity android:label="@string/title_activity_vehicle" android:name=".VehicleActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value="android.app.ListActivity"/>
        </activity>
        <activity android:label="@string/title_activity_new_vehicle" android:name=".NewVehicleActivity"/>
        <activity android:label="@string/title_activity_new_filling" android:name=".EditFillingActivity"/>
        <service android:name=".GCMIntentService"/>
        <receiver android:name="com.google.android.gcm.GCMBroadcastReceiver" android:permission="com.google.android.c2dm.permission.SEND">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE"/>
                <category android:name="de.timoklostermann.refuel"/>
            </intent-filter>
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.REGISTRATION"/>
                <category android:name="de.timoklostermann.refuel"/>
            </intent-filter>
        </receiver>
    </application>

</manifest>